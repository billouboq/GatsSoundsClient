{
  "name": "GatsSounds",
  "version": "1.0.0",
  "description": "Let it rock!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:osx": "electron-packager . trisomie21 --platform=darwin --arch=x64 --version=1.4.11 --app-bundle-id=com.trisomie21 --app-version=1.0.0 --build-version=1.0.100",
    "build:win": "electron-packager . trisomie21 --platform=win32 --arch=x64 --version=1.4.11 --app-bundle-id=com.trisomie21 --app-version=1.0.0 --build-version=1.0.100"
  },
  "dependencies": {
    "electron": "^1.4.1",
    "socket.io-client": "^1.7.2",
    "vue": "^2.1.0",
    "vue-infinite-scroll": "^2.0.0",
    "vue-material": "^0.6.1",
    "vue-router": "^2.1.1",
    "vue-youtube-embed": "^2.0.1",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.26.1",
    "electron-packager": "^8.3.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.7.0",
    "node-sass": "^4.1.1",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.2.0-rc.0",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
